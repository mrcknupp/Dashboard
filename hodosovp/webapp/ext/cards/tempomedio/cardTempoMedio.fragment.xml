<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ovp="sap.ovp.ui"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1">
	<VBox height="336px">
		<VBox alignItems="Center" alignContent="Center" justifyContent="Center" binding="{tempomedio>/}" class="sapUiSmallMargin" height="100%">
			<Title id="ovpTitleTempMedTot" titleStyle="H4" width="100%" text="{path: 'tempomedio>TempMedTot', formatter: '.toHHMMSS'}"/>
			<HBox alignItems="Center" alignContent="Center" justifyContent="Center" class="sapUiSmallMargin">
				<VBox alignItems="Center" alignContent="Center" justifyContent="Center" class="sapUiSmallMarginEnd">
					<Text id="ovpTextQdoVendeu" text="{i18n>txtQdoVendeu}"/>
					<Text id="ovpTextTempMedVend" text="{path: 'tempomedio>TempMedVend', formatter: '.toHHMMSS'}"/>
				</VBox>
				<VBox alignItems="Center" alignContent="Center" justifyContent="Center">
					<Text id="ovpTextQdoNVendeu" text="{i18n>txtQdoNVendeu}"/>
					<Text id="ovpTextTempMedNVend" text="{path: 'tempomedio>TempMedNVend', formatter: '.toHHMMSS'}"/>
				</VBox>
			</HBox>
		</VBox>
	</VBox>
</core:FragmentDefinition>